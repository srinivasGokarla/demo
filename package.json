{
  "name": "demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "cucumber-html-reporter": "^7.2.0",
    "firebase": "^12.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "serve:build": "serve -s build -l 3000",
    "test": "cucumber-js test --require tests/support/hooks.js --exit",
    "deploy": "firebase deploy --only hosting",
    "test:ci": "npx cucumber-js --require cucumber.js --format junit:reports/junit.xml --format progress",
    "test:e2e": "npx cucumber-js --require cucumber.js",
    "test:ci:with-server": "start-server-and-test start http://localhost:3000 test:ci"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cucumber/cucumber": "^12.1.0",
    "@playwright/test": "^1.54.2",
    "@types/node": "^24.2.1",
    "playwright": "^1.54.2",
    "serve": "^14.2.4",
    "start-server-and-test": "^2.0.13"
  }
}
